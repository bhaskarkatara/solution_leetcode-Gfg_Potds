bool containsDigitFour(int num){
     while (num > 0) {
        if (num % 10 == 4) {
            return true;
        }
        num /= 10;
    }
    return false;
}
    int countNumberswith4(int n) {
        // code here
        int count = 0;
    for (int i = 1; i <= n; ++i) {
        if (containsDigitFour(i)) {
            ++count;
        }
    }
    return count;
    }
